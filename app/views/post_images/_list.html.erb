<div class="d-lg-flex d-md-flex my-box-light flex-wrap mt-3">
  <% post_images.each do |post_image| %>
  <div class="my-box-post col-sm-4">
    <%= link_to post_image_path(post_image.id) do %>
      <%= attachment_image_tag post_image, :image, :fill, 200, 200, format: 'jpeg', fallback: "unnamed.jpg", size:'200x200' %>
    <% end %>
    <div class="index mt-3 mb-3">
      <div class="row">
        <div class="col-2">
          
        </div>
        <div class="col-10">
          <p class="card-title">
            <%= link_to(post_image_path(post_image.id), class:"text-dark") do %>
              <strong><i class="far fa-flag"></i><span> <%= post_image.title %></span></strong>
            <% end %>
          </p>
          </div>
      </div>
    </div>
  </div>
  <% end %>
  <%#= paginate post_images %>
</div>